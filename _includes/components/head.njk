<head>
  <meta charset="utf-8">
  <meta name="generator" content="{{ eleventy.generator }}">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>{{ title or settings.title }}</title>
  <meta name="description" content="{{ description or eleventyComputed.description or settings.description }}">
  <!-- Favicon -->
  <link rel="icon" type="image/png" href="https://r2.statstrade.io/assets/icons/favicon-96x96.png" sizes="96x96">
  <link rel="icon" type="image/png" href="https://r2.statstrade.io/assets/icons/favicon-32x32.png" sizes="32x32">
  <link rel="icon" type="image/png" href="https://r2.statstrade.io/assets/icons/favicon-16x16.png" sizes="16x16">
  <link rel="shortcut icon" href="https://r2.statstrade.io/assets/icons/favicon-96x96.png">
  
  {% set css %}
    {% include "assets/css/inline.css" %}
  {% endset %}
  <style>{{ css | cssmin | safe }}</style>
  {# Uncomment below if you want to include minified inline JS #}
  {# {% set js %}
    {% include "assets/js/inline.js" %}
  {% endset %}
  <script>{{ js | jsmin | safe }}</script> #}
</head>
